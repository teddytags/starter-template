{
  "root": true,
  "env": {
    "browser": true,
    "shared-node-browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": ["babel"],
  "extends": "eslint:recommended",
  "rules": { "no-console": 1 }
}
